{
  "name": "mimus",
  "version": "0.2.4",
  "description": "A test runner agnostic stubbing library for Node.JS, supporting dependency injection and auto stubbing.",
  "main": "lib/mimus.js",
  "scripts": {
    "lint-cov": "bin/lint-cov",
    "dev": "bin/dev",
    "test": "bin/test unit && bin/test system",
    "test-cov": "bin/test-cov",
    "test-ci-build": "bin/test-ci-build",
    "compile": "bin/compile",
    "compile-dev": "bin/compile-dev",
    "release-it": "release-it",
    "vile": "vile p -usi -x src.coffee:lib.js -n"
  },
  "engines": {
    "node": ">= 0.12",
    "npm": ">= 2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brentlintner/mimus.git"
  },
  "keywords": [
    "mocking",
    "testing",
    "mock",
    "test",
    "spy",
    "stub",
    "stubbing",
    "dependency",
    "injection"
  ],
  "author": "Brent Lintner <brent.lintner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brentlintner/mimus/issues"
  },
  "homepage": "https://github.com/brentlintner/mimus",
  "dependencies": {
    "coffee-script": "^1.12.4",
    "esprima": "^3.1.3",
    "lodash": "^4.17.4",
    "minilog": "^3.1.0",
    "resolve": "^1.3.2",
    "rewire": "^2.5.2",
    "sinon": "^2.1.0"
  },
  "devDependencies": {
    "@forthright/vile": ">=0.12.0",
    "@forthright/vile-coffeelint": ">=0.1.14",
    "@forthright/vile-comment": ">=0.1.17",
    "@forthright/vile-coverage": ">=0.2.14",
    "@forthright/vile-depcheck": ">=0.1.12",
    "@forthright/vile-eclint": ">=0.1.10",
    "@forthright/vile-git": ">=0.5.1",
    "@forthright/vile-language": ">=0.1.12",
    "@forthright/vile-length": ">=0.1.12",
    "@forthright/vile-license": ">=0.3.14",
    "@forthright/vile-ncu": ">=0.2.1",
    "@forthright/vile-newline": ">=0.1.12",
    "@forthright/vile-nsp": ">=0.2.13",
    "@forthright/vile-stat": ">=0.0.14",
    "@forthright/vile-whitespace": ">=0.1.11",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "npm-check-updates": "^2.10.4",
    "release-it": "^2.6.1",
    "sinon-chai": "^2.9.0",
    "wrench": "^1.5.9"
  }
}
